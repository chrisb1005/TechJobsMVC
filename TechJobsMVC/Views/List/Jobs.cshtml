@using TechJobsMVC.Models
@model IEnumerable<TechJobsMVC.Models.Job>






<h1>@ViewBag.Title</h1>

<div class="container body-content" id="job-listing">

    @foreach (var job in ViewBag.jobs)
    {

        <table class="table table-condensed table-hover">



            <thead>


                <tr>
                <tr>
                <tr></tr>
                <tr></tr>
                <tr></tr>
                <tr></tr>

            </thead>
            <tbody>


                <tr>
                    <th>ID:</th>
                    <td>@job.Id</td>
                </tr>
                <tr>
                    <th>Name:</th>
                    <td>@job.Name</td>

                </tr>

                <tr>
                    <th>employer:</th>
                    <td>@job.Employer</td>
                </tr>
                <tr>
                    <th>location:</th>
                    <td>@job.Location</td>
                </tr>
                <tr>
                    <th>positon type:</th>
                    <td>@job.PositionType</td>
                </tr>
                <tr>
                    <th>core:</th>
                    <td>@job.CoreCompetency</td>
                </tr>


            </tbody>
        </table>



    }
</div>